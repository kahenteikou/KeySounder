<Window x:Class="KeySounder.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KeySounder"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Initialized="Window_Initialized" Closed="Window_Closed">
    <Grid>
        <Button x:Name="StartButton" HorizontalAlignment="Left" Height="27" Margin="54,337,0,0" VerticalAlignment="Top" Width="82" Content="Start" Click="StartButton_Click"/>
        <Button x:Name="StopButton" Content="Stop" HorizontalAlignment="Left" Height="19" Margin="452,344,0,0" VerticalAlignment="Top" Width="76" Click="StopButton_Click"/>

    </Grid>
</Window>
